  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "dev:server": "NODE_ENV=development node server/index.js",

    "// --- Primary Test Commands ---": "--------------------------------",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
